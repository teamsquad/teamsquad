body {
  padding: 0;
  margin: 0;
  text-align: center;
}

#mast {

}

  #search {
    position: fixed;
    top: 6.25em;
    left: 0;
    right: 0;
    z-index: 16;
  }

#content {
  padding: 11em 0 5em 0;
  overflow: hidden;
  background: #FFF;
  min-height: 400px;
}

  div#primary {
    clear: both;
    position: relative;
    width: 65%;
    float: left;
    z-index: 12;
  }

  div#secondary {
    position: fixed;
    top: 11em;
    right: 0;
    left: 0;
    z-index: 11;
  }
  
#foot {
  clear: both;
}

  #foot p {
    margin: 0;
  }

div.inner,
h1 span,
#search form,
#navigation {
  display: block;
  padding: 0 64px 0 64px;
  margin: 0 auto;
  text-align: left;
  position: relative;
  max-width: 1024px;
  min-width: 320px;
  overflow: visible;
}


div.groups {
  overflow: hidden;
}

  div.league_stage_with_groups div.group {
    float: left;
    width: 48%;
  }
  
	div.league_stage_with_groups div.odd {
    clear: both;
  }

  div.league_stage_with_groups div.even {
    float: right;
  }