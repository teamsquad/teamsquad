body {
  background: #FFF;
  color: #555;
  padding: 0;
  margin: 0;
  text-align: center;
}

#mast {

}

  #search {
    position: fixed;
    top: 1.5em;
    left: 0;
    right: 0;
    z-index: 16;
  }
  
  #admin {
    position: absolute;
    right: 0;
    top: 0.5em;
  }

#content {
  padding: 7em 0 6em 0;
  overflow: hidden;
}

  div#primary {
    clear: both;
    position: relative;
    width: 64%;
    float: left;
    z-index: 12;
  }

  div#secondary {
    position: fixed;
    top: 7em;
    right: 0;
    left: 0;
    z-index: 11;
  }
  
#foot {
  clear: both;
  padding: 0.5em 0 0.5em 0;
  font-size: smaller;
  border-top: 2px solid #DDD;
  background: #333;
  color: #EEE;
  position: fixed;
  z-index: 20;
  bottom: 0;
  left: 0;
  right: 0;
}

  #foot p {
    margin: 0;
  }

div.inner,
h1 span,
#search form,
#menu {
  display: block;
  padding: 0 14px 0 64px;
  margin: 0 auto;
  text-align: left;
  position: relative;
  max-width: 1024px;
  min-width: 320px;
  overflow: visible;
}


div.groups {
  overflow: hidden;
}

  div.league_stage_with_groups div.group {
    float: left;
    width: 48%;
  }
  
	div.league_stage_with_groups div.odd {
    clear: both;
  }

  div.league_stage_with_groups div.even {
    float: right;
  }